views:
  - type: table
    name: 表格
    filters:
      and:
        - file.ext == "md"
    order:
      - file.name
      - file.mtime
      - file.ctime
      - file.folder
    sort:
      - property: file.name
        direction: DESC
      - property: file.mtime
        direction: DESC
  - type: table
    name: 视图
